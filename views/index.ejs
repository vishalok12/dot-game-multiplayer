            <div id="overlay">
                <div id="game-over">
                    <button id="game-start-btn">Start New Game</button>
                </div>
                <div id="game-link">
                    <div><span>Send this link to your opponent</span></div>
                    <input class="opponent-game-link" value="" disabled>
                </div>
                <div id="input-wrapper" class="input-group">
                    <input id="user-name" type="text" class="form-control" placeholder="Enter Your Name">
                    <span class="input-group-btn">
                        <button id="submit-btn" class="btn btn-success" type="button">Done</button>
                    </span>
                </div>
            </div>

            <div id="wrapper">
                <canvas width="480" height="320" id="game-area">Your browser doesn't support canvas</canvas>
            </div>
            <!-- socket.io -->
            <script src="/socket.io/socket.io.js"></script>
            <!-- vendor js -->
            <script type="text/javascript" src="js/underscore.js"></script>

            <!-- game js files -->
            <script type="text/javascript" src="js/blockgraph.js"></script>
            <script type="text/javascript" src="js/bot.js"></script>
            <script type="text/javascript" src="js/app.js"></script>

